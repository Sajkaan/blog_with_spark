{{#partial "content"}}
    <section>
        <div class="container">
            <div class="entry-list">
                {{#each blogEntry}}
                    <article>
                        <h1><a href="/detail/{{ slug }}">{{ title }}</a></h1>
                        <h2> {{ author }}</h2>
                        <time>{{ date }}</time>
                        <p> {{ blogPost }}</p>
                    </article>
                {{/each}}
            </div>
        </div>
    </section>
{{/partial}}

{{> base.hbs}}